<!DOCTYPE html>
<html lang="en">
<head>

  {{> head }}

</head>
<body class="{{ pageId }}">
  <div class="wrapper">

    {{> header }}

    <div class="container bs-docs-container">
      <div class="row">

        <main class="col-md-9" role="main">
          {{> body }}
        </main>

        <aside class="col-md-3" role="complementary">

          {{#is pageId 'css'}}
            {{> docsNav nav_css.nav }}
          {{/is}}

          {{#is pageId 'components'}}
            {{> docsNav nav_components.nav }}
          {{/is}}

          {{#is pageId 'js'}}
            {{> docsNav nav_js.nav }}
          {{/is}}

          {{#is pageId 'extras'}}
            {{> docsNav nav_extras.nav }}
          {{/is}}

        </aside>

      </div>
    </div>

    {{> footer }}

  </div>
{{> scripts }}
</body>
</html>
